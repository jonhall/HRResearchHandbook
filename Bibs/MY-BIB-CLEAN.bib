@rejected{hall2019business-analytics,
	author = {Hall, Jon G. and Rapanotti, Lucia},
	date = {2019-02-22},
	date-added = {2019-02-22 15:57:26 +0000},
	date-modified = {2020-10-01 09:58:03 +0200},
	title = {Business analytics as organizational problem solving: an application in {Higher Education}},
	venue = {Journal of Business Analytics},
	year = {2019},
	abstract = {As the adoption of Business Analytics (BA) is growing, an open challenge remains as to how to maximise their value delivery, in particular in helping organisations address their complex and volatile problems. Lack of relevant methodologies is an acknowledged barrier, alongside key skills shortage and other organisational cultural traits. To address this gap, we propose an approach to BA which marries data analytics with an established complex problem solving framework with a track record of successful organisational application. We provide an account of its recent use within a UK higher education institution, and reflect on how it has contributed to increasing the institution's BA readiness and maturity.},
