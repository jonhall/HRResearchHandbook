@article{Hall2005,
	author = {Hall, Jon G. and Rapanotti, Lucia and Jackson, Michael},
	date-added = {2005-06-03 20:33:42 +0100},
	date-modified = {2022-11-21 10:09:37 +0000},
	journal = {Journal of Software and Systems Modeling},
	keywords = {journal},
	local-url = {file://localhost/Users/jon/Documents/Research/Filed%20Papers/Hall/Hall2005.pdf},
	number = {2},
	pages = {189 -- 198},
	title = {Problem Frame semantics for software development},
	url = {http://dx.doi.org/10.1007/s10270-004-0062-1},
	volume = {4},
	year = {2005},
	abstract = {This paper presents a framework for understanding Problem Frames, their components, their frame concerns, and the flexibility that is inherent in their definition. Problem Frames classify software development problems. Of particular utility is their intuitive graphical notation that facilitates communication between software designer and problem owner. This provides costs as well as benefits and, as is the case with many such graphical notations, a (formal) semantics is needed to underpin meaning. To the best of our knowledge a semantics of Problem Frames is missing from the literature. In this paper we begin the definition of such a semantics.

Our semantics places Problem Frames within the framework for Requirements Engineering of Zave and Jackson and its subsequent formalization in the Reference Model of Gunter et al.
},
	rss-description = {Problem frame semantics for software development

Jon G. Hall and Lucia Rapanotti and Michael Jackson

This paper presents a framework for understanding Problem Frames, their components, their frame concerns, and the flexibility that is inherent in their definition. Problem Frames classify software development problems. Of particular utility is their intuitive graphical notation that facilitates communication between software designer and problem owner. This provides costs as well as benefits and, as is the case with many such graphical notations, a (formal) semantics is needed to underpin meaning. To the best of our knowledge a semantics of Problem Frames is missing from the literature. In this paper we begin the definition of such a semantics.

Our semantics places Problem Frames within the framework for Requirements Engineering of Zave and Jackson and its subsequent formalization in the Reference Model of Gunter et al.
