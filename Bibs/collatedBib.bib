@article{kaisti_agile_2013,
	author = {Kaisti, Matti and Rantala, Ville and Mujunen, Tapio and Hyrynsalmi, Sami and K{\"o}nn{\"o}l{\"a}, Kaisa and M{\"a}kil{\"a}, Tuomas and Lehtonen, Teijo},
