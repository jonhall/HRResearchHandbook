	pages = {1--278},
	shorttitle = {{ISO}/{IEC}/{IEEE} {International} {Standard} - {Systems} and software engineering--{Software} life cycle processes--{Part} 2},
	title = {{ISO}/{IEC}/{IEEE} {International} {Standard} - {Systems} and software engineering--{Software} life cycle processes--{Part} 2: {Relation} and mapping between {ISO}/{IEC}/{IEEE} 12207:2017 and {ISO}/{IEC} 12207:2008},
