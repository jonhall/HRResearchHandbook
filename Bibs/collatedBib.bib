	eprint = {0803.1716},
	file = {:C$\backslash$:/Users/samantha/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Obrien - 2010 - The development and evaluation of a survey to measure user engagement.pdf:pdf},
	isbn = {1532-2890},
	issn = {15322890},
