	file = {IEEE Xplore Abstract Record:C\:\Users\Richard\Zotero\storage\52DPC8U7\1231450.html:text/html;IEEE Xplore Full Text PDF:C\:\Users\Richard\Zotero\storage\XDP72XS3\Boehm and Turner - 2003 - Observations on balancing discipline and agility.pdf:application/pdf},
