@misc{Maibauma,
	author = {Maibaum, Tom},
	date-added = {2006-10-09 11:22:46 +0100},
	date-modified = {2007-03-30 09:29:13 +0100},
	local-url = {file://localhost/Users/jon/Documents/Research/Filed%20Papers/Maibaum/Maibauma.pdf},
	title = {How Engineering Extends the Statement View},
	abstract = {We adapt the Carnapian framework for science, the so called Statement View, to describe the 
normative framework of engineering. In particular, we use the terminology of the Statement View 
to explain the process by which an engineer, in a conventional design activity, envisions a new 
artefact in terms of a hypothetical extension of the existing reality, as described by the underlying 
science. The language ohf the underlying science incorporates sublanguages for referring to 
observables and theoretical terms. The hypothetical extension of the underlying science adds new 
observables and, potentially, new theoretical terms. The process of designing and creating the new 
artefact and the extended, new reality in which it exists, may be seen as a systematic manipulation 
of this hypothesised reality to describe the various artefacts of design and the hypothesised artefact 
itself and their relationships to each other and to the engineering process itself. This `altered 
reality' view of engineering may also be seen, in meta theoretical terms, as the systematic manipulation of the Ramsey sentence corresponding to the hypothesised artefact. },
	bdsk-file-1 = {}}

@article{Poel2006a,
	author = {van de Poel, Ibo and van Gorp, A. C.},
	date-added = {2006-10-09 11:06:44 +0100},
	date-modified = {2007-03-30 09:29:13 +0100},
	journal = {Science,Technology, & Human Values},
	local-url = {file://localhost/Users/jon/Documents/Research/Filed%20Papers/Poel/Poel2006a.pdf},
	number = {3},
	pages = {333-360},
	title = {The Need for Ethical Reflection in Engineering Design The Relevance of Type of Design and Design Hierarchy},
	volume = {31},
	year = {2006},
	abstract = {The authors explore whether the need for ethical reflection on the part of 
designing engineers is dependent on the type of design process. They use 
Vincenti's (1992) distinction between normal and radical design and different 
levels of design hierarchy. These two dimensions are coupled with the concept 
of ill-structured problems, which are problems in which possible solutions 
cannot be ordered on a scale from better to worse. Design problems are better 
structured at lower hierarchical levels and in cases of normal design. Better 
structured design problems require less ethical reflection on the part of 
designing engineers if such situations are characterized by the existence of 
generally accepted normative frameworks. Engineers could then deal with 
moral problems within the bounds of such frameworks and without the need 
for further reflection. On the basis of a number of empirical cases,the authors 
explore whether these ideas hold water. They discuss four cases ranging from 
a high-level radical design process to a low-level normal design process.},
	bdsk-file-1 = {}}

