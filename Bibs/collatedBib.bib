@article{picard2004affective,
	author = {Picard, Rosalind W. and Papert, Seymour and Bender, Walter and Blumberg, Bruce and Breazeal, Cynthia and Cavallo, David and Machover, Tod and Resnick, Mitchel and Roy, Deb and Strohecker, Carol},
