@article{DMello2012b,
	author = {D'Mello, Sidney K. and Graesser, Art},
	date-added = {2016-04-13 08:31:45 +0000},
	date-modified = {2016-04-13 08:31:45 +0000},
	doi = {10.1109/TLT.2012.10},
	file = {:C$\backslash$:/Users/samantha/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/D'Mello, Graesser - 2012 - Language and Discourse Are Powerful Signals of Student Emotions during Tutoring.pdf:pdf},
	issn = {1939-1382},
	journal = {IEEE Transactions on Learning Technologies},
	keywords = {Behavioral science,Education,Emotion recognition,Emotions,Feature extraction,ITS,MLR models,Natural language processing,Particle measurements,Pragmatics,Psychology,Semantics,affect,affect from text,cohesion-based methods,conversational dialogues,direct expressions,intelligent tutoring system,intelligent tutoring systems,interactive systems,learning session,linguistic terms,natural language processing,natural language processing techniques,powerful signals,psychological terms,regression analysis,semantic alignment methods,semantic content,student emotions,student responses,text analysis,text-based emotion detection,textual features,trained judges,tutorial dialogues,untrained peers},
	month = oct,
	number = {4},
	pages = {304--317},
	shorttitle = {Learning Technologies, IEEE Transactions on},
	title = {{Language and Discourse Are Powerful Signals of Student Emotions during Tutoring}},
	volume = {5},
	year = {2012},
	abstract = {We explored the possibility of predicting student emotions (boredom, flow/engagement, confusion, and frustration) by analyzing the text of student and tutor dialogues during interactions with an Intelligent Tutoring System (ITS) with conversational dialogues. After completing a learning session with the tutor, student emotions were judged by the students themselves (self-judgments), untrained peers, and trained judges. Transcripts from the tutorial dialogues were analyzed with four methods that included 1) identifying direct expressions of affect, 2) aligning the semantic content of student responses to affective terms, 3) identifying psychological and linguistic terms that are predictive of affect, and 4) assessing cohesion relationships that might reveal student affect. Models constructed by regressing the proportional occurrence of each emotion on textual features derived from these methods yielded large effects (R2 = 38\%) for the psychological, linguistic, and cohesion-based methods, but not the direct expression and semantic alignment methods. We discuss the theoretical, methodological, and applied implications of our findings toward text-based emotion detection during tutoring.},
	bdsk-url-1 = {http://dx.doi.org/10.1109/TLT.2012.10}}

