	file = {Full Text PDF:C\:\Users\Richard\Zotero\storage\5WZD83Z9\Zasa et al. - 2021 - Managing the Hybrid Organization How Can Agile an.pdf:application/pdf;Snapshot:C\:\Users\Richard\Zotero\storage\IHZ8DVXY\08956308.2021.html:text/html},
	issn = {0895-6308},
