	issn = {1937-4194},
	journal = {IEEE Software},
	keywords = {software process improvement, ISO standards, Resource management, Capability maturity model, Capability Maturity Model, continuous improvement, Coordinate measuring machines, Distributed software development, distributed software project management, key process areas, Knowledge management, Programming, project management, Project management, software development management, Software development management, software development process, Software engineering, Software testing},
