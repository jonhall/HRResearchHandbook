	keywords = {ViewPoint integration complementary participants composite system development configuration management development process knowledge formal specification inter-ViewPoint communication knowledge representation locally managed object method application stage method construction stage multiple views object-oriented programming partial specification knowledge representation knowledge requirements specification ViewPoint integration complementary participants composite system development configuration management development process knowledge formal specification inter-ViewPoint communication knowledge representation locally managed object method application stage method construction stage multiple views object-oriented programming partial specification knowledge representation knowledge requirements specification},
	note = {TY - CONF},
