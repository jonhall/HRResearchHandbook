	date-added = {2021-06-07 16:38:06 +0100},
	date-modified = {2021-06-07 16:42:16 +0100},
	doi = {https://doi.org/10.1016/S1361-3723(16)30025-2},
	issn = {1361-3723},
	journal = {Computer Fraud \& Security},
	keywords = {Online Recruitment Fraud},
	number = {3},
	pages = {8-13},
	title = {Online recruitment services: another playground for fraudsters},
	url = {https://www.sciencedirect.com/science/article/pii/S1361372316300252},
	volume = {2016},
	year = {2016},
	abstract = {It is increasingly common for someone actively seeking a job online to come across appealing but fake ads offering high wages, flexible hours, teleworking and career growth opportunities. Usually, job ads with such favourable conditions are examples of Online Recruitment Fraud (ORF) which attempt to collect unsuspecting candidates' personal information. ORF is a relatively new field of variable severity that can escalate quickly to extensive scams. Corporate hiring has recently been ported to the cloud. However, this has fuelled a new type of online scam that jeopardises job seekers' privacy and harms the reputation of organisations. Employment scams share common characteristics with others, such as email spam and phishing, but also have their own peculiarities. Sokratis Vidros and Georgios Kambourakis of the University of the Aegean, Greece and Constantinos Kolias of George Mason University, US present a preliminary analysis of real-life fraudulent job ads.},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S1361372316300252},
	bdsk-url-2 = {https://doi.org/10.1016/S1361-3723(16)30025-2}}

