@inproceedings{10.1007/11693017_16,
	address = {Berlin, Heidelberg},
	author = {Lavazza, Luigi and Del Bianco, Vieri},
	booktitle = {Fundamental Approaches to Software Engineering},
	date-added = {2023-10-11 15:27:09 +0100},
	date-modified = {2023-10-11 15:27:09 +0100},
	editor = {Baresi, Luciano and Heckel, Reiko},
	isbn = {978-3-540-33094-3},
	pages = {199--213},
	publisher = {Springer Berlin Heidelberg},
	title = {Combining Problem Frames and UML in the Description of Software Requirements},
	year = {2006},
	abstract = {Problem frames are a sound and convenient approach to requirements modeling. Nevertheless, they are far less popular than other less rigorous approaches. One reason is that they employ a notation that is neither very appealing nor easy to use. The problem frames notation is sufficiently different from other development languages --especially UML-- to create an \enquote{impedance mismatch}: using problem frames to describe requirements does not help the transition to the design phase, makes it difficult for programmers to fully comprehend requirements, and does not favor traceability. As a consequence, problem frames are rarely adopted in software development processes employing UML as a design language. UML itself provides a linguistic support for requirements modeling, which however suffers from several limitations, especially as far as precision and formality are concerned.}}

