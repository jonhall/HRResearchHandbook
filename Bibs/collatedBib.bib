	issn = {1098-7584},
	keywords = {Adaptation model,Artificial intelligence,Databases,Humans,Materials,Oscar system,Pattern matching,Tutorials,conversational agent,intelligent tutoring systems,learning style estimation,software agents,tutoring conversation},
	month = jul,
