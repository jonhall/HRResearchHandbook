	keywords = {Scrum, global software development, Resource management, Software, Companies, software prototyping, Global Software Development, agile development approach, Agile Practices, Contracts, Cultural differences, Empirical Study, Europe, GSD company, Interviews, Motivation, plan driven approach, planning, requirement ambiguity, unpredictable customer expectations, waterfall development process},
