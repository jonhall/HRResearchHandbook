	doi = {10.1108/IJMPB-06-2020-0187},
	file = {Full Text PDF:C\:\Users\Richard\Zotero\storage\QM75VVGA\Costantini et al. - 2021 - Using complexity and volatility characteristics to.pdf:application/pdf},
