@article{weick1999organizational,
	author = {Weick, Karl E. and Quinn, Robert E.},
	date-added = {2024-09-19 10:45:41 +0100},
	date-modified = {2024-09-19 11:32:52 +0100},
	issn = {1545-2085},
	journal = {Annual Review of Psychology},
	keywords = {adaptation},
	number = {Volume 50, 1999},
	pages = {361-386},
	publisher = {Annual Reviews},
	title = {ORGANIZATIONAL CHANGE AND DEVELOPMENT},
	url = {https://www.annualreviews.org/content/journals/10.1146/annurev.psych.50.1.361},
	volume = {50},
	year = {1999},
	abstract = {Abstract  Recent analyses of organizational change suggest a growing concern with the tempo of change, understood as the characteristic rate, rhythm, or pattern of work or activity. Episodic change is contrasted with continuous change on the basis of implied metaphors of organizing, analytic frameworks, ideal organizations, intervention theories, and roles for change agents. Episodic change follows the sequence unfreeze-transition-refreeze, whereas continuous change follows the sequence freeze-rebalance-unfreeze. Conceptualizations of inertia are seen to underlie the choice to view change as episodic or continuous.},
	bdsk-url-1 = {https://www.annualreviews.org/content/journals/10.1146/annurev.psych.50.1.361},
