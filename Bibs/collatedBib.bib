@article{Koutropoulos2012,
	author = {Koutropoulos, Apostolos and Gallagher, Michael Sean and Abajian, Sean C and de Waard, Inge and Hogue, Rebecca Joanne and Keskin, Nilgun Ozdamar and Rodriguez, Osvaldo C},
