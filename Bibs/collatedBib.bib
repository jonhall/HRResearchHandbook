	file = {Springer Full Text PDF:C\:\Users\Richard\Zotero\storage\IQWED7G2\Hanssen et al. - 2017 - An Assessment of Avionics Software Development Pra.pdf:application/pdf},
	isbn = {978-3-319-57633-6},
	keywords = {Scrum, Tailoring, Agile, Avionics, Certification, DO-178, Safety critical software, Software Life-Cycle},
	language = {en},
