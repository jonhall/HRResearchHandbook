	doi = {10.1016/j.ijpe.2020.107853},
	file = {ScienceDirect Full Text PDF:C\:\Users\Richard\Zotero\storage\EICT9QF3\Kamble et al. - 2020 - A performance measurement system for industry 4.0 .pdf:application/pdf},
	issn = {0925-5273},
